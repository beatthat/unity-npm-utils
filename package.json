{
    "name": "unity-npm-utils",
    "scope": "beatthat",
    "version": "1.0.60",
    "description": "utils for using npm as a package manager for unity",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/beatthat/unity-npm-utils.git"
    },
    "main": "./lib/unity-npm-utils",
    "bin": {
        "unpm": "./bin/unpm",
        "overwrite2clone": "./bin/overwrite2clone",
        "overwrite2src": "./bin/overwrite2src"
    },
    "files": [
        "lib",
        "bin"
    ],
    "author": "larrykirschner@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/beatthat/unity-npm-utils/issues"
    },
    "homepage": "https://github.com/beatthat/unity-npm-utils",
    "scripts": {
        "test": "./node_modules/.bin/mocha --recursive --reporter spec",
        "pretest:unityproject:install": "rm -f *.tgz",
        "test:unityproject:install": "npm pack && cp ${npm_package_name}-${npm_package_version}.tgz ${npm_package_name}-latest.tgz && cd test_unity_project && npm run clean && npm install",
        "clean": "rm -f *.tgz *.log *.swp",
        "version": "git add -A",
        "postversion": "git push && git push --tags && npm run clean"
    },
    "dependencies": {
        "app-root-path": "^2.0.1",
        "async": "^2.6.1",
        "chai-files": "^1.4.0",
        "child-process": "^1.0.2",
        "command-line-args": "^4.0.7",
        "commander": "^2.15.1",
        "dateformat": "^3.0.2",
        "decompress": "^4.2.0",
        "delay": "^2.0.0",
        "download-git-repo": "^1.0.2",
        "es6-promisify": "^5.0.0",
        "fs-extra": "^6.0.1",
        "git-credential-helper": "0.0.1",
        "git-tools": "github:beatthat/node-git-tools",
        "github-api": "^3.0.0",
        "giturl": "^1.0.0",
        "graceful-ncp": "^3.0.0",
        "home-or-tmp": "^3.0.0",
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0",
        "node-unity-guid-utils": "github:beatthat/node-unity-guid-utils",
        "request": "^2.87.0",
        "rimraf": "^2.6.2",
        "semver": "^5.4.1",
        "shell-escape": "^0.2.0",
        "smart-spawn": "^2.0.1",
        "tmp": "0.0.33",
        "tmp-promise": "^1.0.3",
        "yamljs": "^0.3.0"
    },
    "engines": {
        "node": "*"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^3.5.3",
        "mocha-logger": "^1.0.6"
    }
}
